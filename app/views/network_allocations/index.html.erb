<h1>Listing network_allocations</h1>

<table>
  <tr>
    <th>Collection</th>
    <th>Network address</th>
  </tr>

<% @network_allocations.each do |network_allocation| %>
  <tr>
    <td><%=h network_allocation.collection_id %></td>
    <td><%=h network_allocation.network_address %></td>
    <td><%= link_to 'Show', network_allocation %></td>
    <td><%= link_to 'Edit', edit_network_allocation_path(network_allocation) %></td>
    <td><%= link_to 'Destroy', network_allocation, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New network_allocation', new_network_allocation_path %>